    // Print sectie header
    private static void PrintSectionHeader(string title)
    {
        // ==== Start of Function ====
        Console.ForegroundColor = ConsoleColor.Green;
        Console.WriteLine($"\n==================================================");
        Console.WriteLine($"  {title}");
        Console.WriteLine($"==================================================");
        Console.ResetColor();
    }

