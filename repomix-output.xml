            return Ok(types);
        }

        // ============================================================
        // ==== LOGBOEK ENDPOINTS ====
        // Read-only endpoints voor de audit trail
        // ============================================================

        // GET: api/gite/logs?count=50
        [HttpGet("logs")]
        public async Task<ActionResult<List<LogboekDTO>>> GetRecentLogs([FromQuery] int count = 50)
        {
            // ==== Start of Function ====
            var logs = await _repository.GetRecentLogsAsync(count);
            return Ok(logs);
        }
    }
}
